<% if (typeof errors != 'undefined') { %>
	<% errors.forEach(function(error) { %>
		<div class="container">
			<div class="row">
				<div class="col">
					<div class="alert alert-danger"><%= error.msg %></div>
				</div>
			</div>
		</div>
	<% }); %>
<% } %>

<% if (success_msg != '') { %>
	<div class="container">
		<div class="row">
			<div class="col">
				<div class="alert alert-danger"><%= success_msg %></div>
			</div>
		</div>
	</div>
<% } %>

<% if (error_msg != '') { %>
	<div class="container">
		<div class="row">
			<div class="col">
				<div class="alert alert-danger"><%= error_msg %></div>
			</div>
		</div>
	</div>
<% } %>

<% if (error != '') { %>
	<div class="container">
		<div class="row">
			<div class="col">
				<div class="alert alert-danger"><%= error %></div>
			</div>
		</div>
	</div>
<% } %>

<% if (message != '') { %>
	<div class="container">
		<div class="row">
			<div class="col">
				<div class="alert alert-danger"><%= message %></div>
			</div>
		</div>
	</div>
<% } %>