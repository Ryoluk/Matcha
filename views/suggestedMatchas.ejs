<head>
	<%- include('./default/head'); -%>
</head>
<header>
	<%- include('./default/profileHeader'); -%>
</header>
<main>
	<%- include('./partials/messages'); -%>
	<%- include('./forms/search') -%>
    <br><h3 class="text-center text-danger">Suggested Matcha's</h3><br>

	<div class="container table-responsive">
		<table class="table table-hover">
			<% if (typeof users !== 'undefined') { %> 
				<% users.forEach(function (user) { %>
					<tr onclick="window.location='<%= user.request.url %>'">
							<td class="image">
							<div class="col-lg-3 col-md-4 col-6">
									<img src="<%= user.profileImage %>" alt="" style="height: 100%; width: 150%">
							</div>
						</td>
						<td class="product text-left"><strong><%= user.firstname %> <%= user.lastname %></strong><br><%= user.username %></td>
						<td class="rate text-right"><span><i class="fa fa-star"></i><i class="fa fa-star"></i><i class="fa fa-star"></i><i class="fa fa-star"></i><i class="fa fa-star-half-o"></i></span><br>Popularity</td>
					</tr>
				<% })
				} %>
			</table>
	</div>
</main>
<footer>
	<%- include('./default/footer'); -%>
</footer>